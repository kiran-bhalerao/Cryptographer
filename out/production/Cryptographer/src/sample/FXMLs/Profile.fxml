<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="540.0" prefWidth="520.0" style="-fx-background-color: #dddddd;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121" fx:controller="sample.Controllers.Profile">
    <children>
      <AnchorPane layoutX="-7.0" prefHeight="139.0" prefWidth="529.0" style="-fx-background-color: #7324e2;" />
        <JFXButton layoutY="480.0" onMouseClicked="#saveClicked" prefHeight="60.0" prefWidth="522.0" style="-fx-background-color: #7324e2; -fx-background-radius: 0;" text="SAVE" textFill="WHITE" />
        <ImageView fitHeight="13.0" fitWidth="13.0" layoutX="482.0" layoutY="19.0" onMouseClicked="#close" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../res/close.png" />
            </image>
        </ImageView>
      <JFXTextField fx:id="username" editable="false" focusColor="#4d0c79" layoutX="99.0" layoutY="226.0" onKeyPressed="#enterKeyusername" prefWidth="320.0" promptText="Username" />
      <JFXTextField fx:id="password" layoutX="100.0" layoutY="290.0" onKeyPressed="#enterKeyPassword" prefWidth="320.0" promptText="Password" />
      <JFXTextField fx:id="cpassword" layoutX="101.0" layoutY="349.0" onKeyPressed="#enterKeyCPassword" prefWidth="320.0" promptText="Confirm Password" />
      <JFXTextField fx:id="nickName" layoutX="102.0" layoutY="411.0" onKeyPressed="#enterKeyNickName" prefWidth="320.0" promptText="Whats your Nick Name ?" />
    </children>
</AnchorPane>
